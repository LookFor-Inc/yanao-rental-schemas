{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Password reset request",
  "type": "object",
  "properties": {
    "token": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 64
    }
  },
  "required": [
    "token",
    "password"
  ],
  "additionalProperties": false
}